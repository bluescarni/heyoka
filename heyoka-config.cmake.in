if(@HEYOKA_WITH_MPPP@)
    find_package(mp++ REQUIRED CONFIG)
    if(${mp++_VERSION} VERSION_LESS @_HEYOKA_MIN_MPPP_VERSION@)
        message(FATAL_ERROR "mp++ >= @_HEYOKA_MIN_MPPP_VERSION@ is required.")
    endif()
endif()

set(heyoka_WITH_SLEEF @HEYOKA_WITH_SLEEF@)
